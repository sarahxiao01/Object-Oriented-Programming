vector<int> vectorOfInts;
for (int i = 10; i <= 100; i+=2) {
    vectorOfInts.push_back(i);
}

for (size_t i = 0; i < vectorOfInts.size(); ++i) {
    cout << vectorOfInts[i] << endl;
}

for (int x : vectorOfInts) {
    cout << x << endl;
}

for (size_t i = vectorOfInts.size(); i > 0 ; --i) {
    cout << vectorOfInts[i-1] << endl;
}

vector<int> v1{2, 3, 5, 7, 8, 11, 13, 17, 19};

for (size_t i = 0; i < v1.size(); ++i) {
    cout << vectorOfInts[i] << endl;
}

void displayVector(vetor<int> myvec) {
